{
  "name": "copy-paste-component",
  "version": "1.0.4",
  "description":
    "A tool to create components, by copying and pasting existing ones.",
  "keywords": [
    "cli",
    "cli-app",
    "frontend",
    "front-end",
    "development",
    "dev",
    "web",
    "generate",
    "generator",
    "component",
    "components",
    "copy",
    "paste",
    "tool",
    "scaffold",
    "app",
    "boilerplate"
  ],
  "author":
    "Gabriel Duarte <gabrielqduarte@gmail.com> (https://github.com/GabrielDuarteM/)",
  "license": "MIT",
  "main": "bin/copy-paste-component.js",
  "homepage": "https://github.com/GabrielDuarteM/copy-paste-component",
  "files": ["dist"],
  "scripts": {
    "start": "yarn build:dev",
    "prebuild": "yarn clean",
    "build": "babel src -d dist --ignore *.test.js",
    "build:dev": "yarn build --watch -s",
    "clean": "rimraf dist",
    "lint": "eslint src/*.js",
    "test": "jest",
    "test:w": "yarn test --watch",
    "build:ci": "yarn lint && yarn test && yarn build",
    "prepublishOnly": "yarn build:ci"
  },
  "bin": {
    "copy-paste-component": "dist/bin.js",
    "cpc": "dist/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GabrielDuarteM/copy-paste-component.git"
  },
  "bugs": {
    "url": "https://github.com/GabrielDuarteM/copy-paste-component/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "modulePathIgnorePatterns": ["<rootDir>/fixtures/"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-prettier": "^2.4.0",
    "jest": "^22.1.3",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "inquirer": "^5.0.0",
    "klaw-sync": "^3.0.2"
  }
}
